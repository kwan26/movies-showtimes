{
  "manifest_version": 2,
  "background": { "scripts": ["scripts/jquery.min.js","scripts/background.js"] },
  "name": "Movies Showtimes",
  "description": "This extension demonstrates the showtimes near you",
  "version": "1.0.5",
  "permissions": [
    "http://showtimes.tiagomestre.pt/",
    "storage",
    "notifications",
    "background"
  ],
  "icons": { 
		"32": "Popcorn - All-png.png",
		"48": "Popcorn - All-png.png",
		"128": "Popcorn - All-png.png" 
	},
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "Popcorn - All-png.png",
    "default_popup": "popup_1.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}